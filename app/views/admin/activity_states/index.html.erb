<h1>States for <%= @store.title %> </h1>

<%= link_to 'Back', admin_lrs_index_path, class: "btn btn-default btn-xs" %>

<table id="states" class="table table-hover table-striped table-condensed">
  <thead>
  <tr>
    <td>ID</td>
  </tr>
  </thead>

  <% @store.activity_states.each do |state| %>
      <tr>
        <td><%= state.id %></td>
      </tr>
  <% end %>
</table>