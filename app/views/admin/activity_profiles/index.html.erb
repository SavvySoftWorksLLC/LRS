<h1>Activity profiles for <%= @store.title %> </h1>

<%= link_to 'Back', admin_lrs_index_path, class: "btn btn-default btn-xs" %>

<table id="activity_profiles" class="table table-hover table-striped table-condensed">
  <thead>
  <tr>
    <td>ID</td>
  </tr>
  </thead>

  <% @store.activity_profiles.each do |activity| %>
      <tr>
        <td><%= activity.id %></td>
      </tr>
  <% end %>
</table>